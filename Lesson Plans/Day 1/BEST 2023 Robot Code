#pragma config(Sensor, dgtl2,  IR1,            sensorDigitalIn)
#pragma config(Sensor, dgtl6,  IR2,            sensorDigitalIn)
#pragma config(Motor,  port2,           LeftMotor,     tmotorVex393_MC29, openLoop)
#pragma config(Motor,  port9,           RightMotor,    tmotorVex393_MC29, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

task main()
{
	//define the joysticks
	int LeftC = vexRT[Ch3];
	int RightC = vexRT[Ch2];

	while(1==1)
	{
		//redefines joysticks
		LeftC = vexRT[Ch3];
		RightC = vexRT[Ch2];
		{
		        //sets motor speed to joystick
			motor[RightMotor] = RightC;
			motor[LeftMotor] = LeftC;
		}
	}
}
